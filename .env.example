# Client-side configuration (prefixed with VITE_)
VITE_API_URL="http://localhost:3001"

# Server-side configuration (no VITE_ prefix)
AI_PROVIDER="deepseek"
DEEPSEEK_MODEL="deepseek-chat"
DEEPSEEK_API_KEY=""
DEEPSEEK_ENDPOINT=""

MYSQL_ROOT_PASSWORD="mysql-root-password"
MYSQL_USER="mysql-user"
MYSQL_PASSWORD="mysql-password"
DATABASE_URL="mysql://mysql-user:mysql-password@db:3306/spritel"
NODE_ENV="development"

# OpenAI configuration (for fallback)
OPENAI_API_KEY="your-openai-api-key-here"

# Lucia auth configuration
# Name of the HttpOnly session cookie
LUCIA_SESSION_COOKIE_NAME="spritel_session"
# Session TTL in seconds (e.g., 86400 = 1 day)
SESSION_TTL="86400"
